{
  "name": "repocompare",
  "version": "0.1.0",
  "description": "A React powered single page application that you can easily compare Github repositories.",
  "scripts": {
    "start": "webpack-dev-server --hot --inline --progress --colors --watch",
    "build": "webpack -p --progress",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "lint": "eslint --ext .jsx --ext .js app",
    "lint:fix": "npm run lint -- --fix",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tsevdos/react-boilerplate"
  },
  "author": "John Tsevdos <tsevdosjohn@gmail.com> (http://tsevdos.me)",
  "license": "GPL3",
  "bugs": {
    "url": "https://github.com/tsevdos/repocompare/issues"
  },
  "homepage": "http://repocompare.io",
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "css-loader": "0.28.5",
    "enzyme": "2.9.1",
    "eslint": "4.5.0",
    "eslint-config-prettier": "2.3.0",
    "eslint-config-standard": "10.2.1",
    "eslint-plugin-babel": "4.1.2",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-node": "5.1.1",
    "eslint-plugin-prettier": "2.2.0",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-react": "7.2.1",
    "eslint-plugin-standard": "3.0.1",
    "html-webpack-plugin": "2.30.1",
    "jest": "20.0.4",
    "jest-cli": "20.0.4",
    "mobx-react-devtools": "4.2.15",
    "prettier": "1.5.3",
    "style-loader": "0.18.2",
    "webpack": "3.5.5",
    "webpack-dev-server": "2.7.1"
  },
  "dependencies": {
    "aphrodite": "1.2.3",
    "axios": "0.16.2",
    "material-ui": "0.19.0",
    "mobx": "3.2.2",
    "mobx-react": "4.2.2",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-flexbox-grid-aphrodite": "1.0.3",
    "react-router": "3.0.0",
    "react-share": "1.15.1",
    "react-tap-event-plugin": "2.0.1",
    "typography": "0.16.0",
    "typography-theme-github": "0.15.10"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "roots": [
      "<rootDir>/app"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/dist/"
    ],
    "modulePaths": [
      "<rootDir>/app/"
    ]
  }
}

{
  "name": "repocompare",
  "version": "0.1.0",
  "description": "A React powered single page application that you can easily compare Github repositories.",
  "scripts": {
    "start": "webpack-dev-server --hot --inline --progress --colors --watch",
    "build": "webpack -p --progress",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "lint": "eslint --ext .jsx --ext .js app",
    "lint:fix": "npm run lint -- --fix",
    "eslint-check": "eslint --print-config .eslintrc | eslint-config-prettier-check",
    "webpack-stats": "webpack --json > stats.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tsevdos/react-boilerplate"
  },
  "author": "John Tsevdos <tsevdosjohn@gmail.com> (http://tsevdos.me)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tsevdos/repocompare/issues"
  },
  "homepage": "http://repocompare.io",
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.3",
    "babel-jest": "22.4.3",
    "babel-loader": "7.1.4",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "css-loader": "0.28.11",
    "enzyme": "3.3.0",
    "eslint": "4.19.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-config-standard": "11.0.0",
    "eslint-plugin-babel": "5.1.0",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-node": "6.0.1",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-promise": "3.7.0",
    "eslint-plugin-react": "7.7.0",
    "eslint-plugin-standard": "3.0.1",
    "html-webpack-plugin": "3.2.0",
    "jest": "22.4.3",
    "jest-cli": "22.4.3",
    "prettier": "1.12.1",
    "style-loader": "0.21.0",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.11.2"
  },
  "dependencies": {
    "apollo-client-preset": "1.0.8",
    "apollo-link-context": "1.0.8",
    "graphql": "0.13.2",
    "graphql-tag": "2.9.1",
    "material-ui": "0.20.0",
    "prop-types": "15.6.1",
    "qs": "6.5.1",
    "react": "16.3.2",
    "react-apollo": "2.1.3",
    "react-dom": "16.3.2",
    "react-flexbox-grid": "2.0.0",
    "react-router-dom": "4.2.2",
    "react-share": "2.1.1",
    "react-tap-event-plugin": "3.0.2",
    "typography": "0.16.6",
    "typography-theme-github": "0.15.10"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "roots": [
      "<rootDir>/app"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/dist/"
    ],
    "modulePaths": [
      "<rootDir>/app/"
    ]
  }
}

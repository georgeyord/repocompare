{
  "name": "repocompare",
  "version": "0.1.0",
  "description": "A React powered single page application that you can easily compare Github repositories.",
  "scripts": {
    "start": "webpack-dev-server --hot --inline --progress --colors --watch",
    "build": "webpack -p --progress",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "lint": "eslint --ext .jsx --ext .js app",
    "lint:fix": "npm run lint -- --fix",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "webpack-stats": "webpack --json > stats.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tsevdos/react-boilerplate"
  },
  "author": "John Tsevdos <tsevdosjohn@gmail.com> (http://tsevdos.me)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tsevdos/repocompare/issues"
  },
  "homepage": "http://repocompare.io",
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "css-loader": "0.28.7",
    "enzyme": "3.1.0",
    "eslint": "4.9.0",
    "eslint-config-prettier": "2.6.0",
    "eslint-config-standard": "10.2.1",
    "eslint-plugin-babel": "4.1.2",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-node": "5.2.1",
    "eslint-plugin-prettier": "2.3.1",
    "eslint-plugin-promise": "3.6.0",
    "eslint-plugin-react": "7.4.0",
    "eslint-plugin-standard": "3.0.1",
    "html-webpack-plugin": "2.30.1",
    "jest": "21.2.1",
    "jest-cli": "21.2.1",
    "prettier": "1.7.4",
    "style-loader": "0.19.0",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.3"
  },
  "dependencies": {
    "axios": "0.17.0",
    "axios-cancel": "0.2.2",
    "material-ui": "0.19.4",
    "prop-types": "15.6.0",
    "qs": "6.5.1",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-flexbox-grid": "1.1.5",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "react-share": "1.16.0",
    "react-tap-event-plugin": "3.0.2",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "typography": "0.16.6",
    "typography-theme-github": "0.15.10"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "roots": [
      "<rootDir>/app"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/dist/"
    ],
    "modulePaths": [
      "<rootDir>/app/"
    ]
  }
}

{
  "name": "repocompare",
  "version": "0.1.0",
  "description": "A React powered single page application that you can easily compare Github repositories.",
  "scripts": {
    "start": "webpack-dev-server --mode development --hot --inline --progress --colors --watch",
    "build": "webpack --mode production",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "lint": "eslint --ext .jsx --ext .js app",
    "lint:fix": "npm run lint -- --fix",
    "eslint-check": "eslint --print-config .eslintrc | eslint-config-prettier-check",
    "webpack-stats": "webpack --json > stats.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tsevdos/react-boilerplate"
  },
  "author": "John Tsevdos <tsevdosjohn@gmail.com> (http://tsevdos.me)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tsevdos/repocompare/issues"
  },
  "homepage": "http://repocompare.io",
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.6",
    "babel-jest": "23.4.0",
    "babel-loader": "7.1.5",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "css-loader": "1.0.0",
    "enzyme": "3.3.0",
    "eslint": "5.2.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-config-standard": "11.0.0",
    "eslint-plugin-babel": "5.1.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-node": "7.0.1",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-react": "7.10.0",
    "eslint-plugin-standard": "3.1.0",
    "html-webpack-plugin": "3.2.0",
    "jest": "23.4.1",
    "jest-cli": "23.4.1",
    "prettier": "1.13.7",
    "style-loader": "0.21.0",
    "uglifyjs-webpack-plugin": "1.2.7",
    "webpack": "4.16.2",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.5"
  },
  "dependencies": {
    "apollo-client-preset": "1.0.8",
    "apollo-link-context": "1.0.8",
    "firebase": "5.3.0",
    "graphql": "0.13.2",
    "graphql-tag": "2.9.2",
    "js-cookie": "2.2.0",
    "material-ui": "0.20.1",
    "prop-types": "15.6.2",
    "qs": "6.5.2",
    "react": "16.4.1",
    "react-apollo": "2.1.9",
    "react-dom": "16.4.1",
    "react-flexbox-grid": "2.1.2",
    "react-router-dom": "4.3.1",
    "react-share": "2.2.0",
    "react-tap-event-plugin": "3.0.3"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "roots": [
      "<rootDir>/app"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/dist/"
    ],
    "modulePaths": [
      "<rootDir>/app/"
    ]
  }
}
